{
  "projectType": "library",
  "root": "libs/apps/admin",
  "sourceRoot": "libs/apps/admin/src",
  "prefix": "lib-admin",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/apps/admin"],
      "options": {
        "jestConfig": "libs/apps/admin/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/apps/admin/src/**/*.ts",
          "libs/apps/admin/src/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/angular",
        "port": 4400,
        "config": {
          "configFolder": "libs/apps/admin/.storybook"
        },
        "projectBuildConfig": "apps-admin:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/angular",
        "outputPath": "dist/storybook/apps-admin",
        "config": {
          "configFolder": "libs/apps/admin/.storybook"
        },
        "projectBuildConfig": "apps-admin:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": ["platform:angular", "layer:app-logic", "scope:frontend"]
}
