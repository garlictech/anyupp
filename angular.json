{
  "version": 1,
  "projects": {
    "admin": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/admin",
      "sourceRoot": "apps/admin/src",
      "prefix": "bgap",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/admin",
            "index": "apps/admin/src/index.html",
            "main": "apps/admin/src/main.ts",
            "polyfills": "apps/admin/src/polyfills.ts",
            "tsConfig": "apps/admin/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/admin/src/favicon.ico", "apps/admin/src/assets"],
            "styles": [
              "apps/admin/src/styles.scss",
              "node_modules/@nebular/theme/styles/prebuilt/default.css",
              "node_modules/bootstrap/dist/css/bootstrap.css"
            ],
            "allowedCommonJsDependencies": [
              "fabric",
              "chart.js",
              "chartjs-plugin-datalabels",
              "lodash",
              "firebase/app",
              "@firebase",
              "@firebase/app",
              "@firebase/auth",
              "@firebase/component",
              "@firebase/database",
              "@firebase/functions",
              "@angular/common/locales"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/admin/src/environments/environment.ts",
                  "with": "apps/admin/src/environments/environment.prod.ts"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/firebase.config.json",
                  "with": "libs/shared/config/src/lib/config/prod/firebase.config.prod.json"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/stripe.config.json",
                  "with": "libs/shared/config/src/lib/config/prod/stripe.config.prod.json"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/service-account-key.json",
                  "with": "libs/shared/config/src/lib/config/prod/service-account-key.prod.json"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "qa": {
              "fileReplacements": [
                {
                  "replace": "apps/admin/src/environments/environment.ts",
                  "with": "apps/admin/src/environments/environment.qa.ts"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/firebase.config.json",
                  "with": "libs/shared/config/src/lib/config/qa/firebase.config.qa.json"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/stripe.config.json",
                  "with": "libs/shared/config/src/lib/config/qa/stripe.config.qa.json"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/service-account-key.json",
                  "with": "libs/shared/config/src/lib/config/qa/service-account-key.qa.json"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "apps/admin/src/environments/environment.ts",
                  "with": "apps/admin/src/environments/environment.staging.ts"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/firebase.config.json",
                  "with": "libs/shared/config/src/lib/config/staging/firebase.config.staging.json"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/stripe.config.json",
                  "with": "libs/shared/config/src/lib/config/staging/stripe.config.staging.json"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/service-account-key.json",
                  "with": "libs/shared/config/src/lib/config/staging/service-account-key.staging.json"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "admin:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "admin:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/admin/**/*.ts", "apps/admin/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/admin/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/admin"]
        }
      }
    },
    "admin-e2e": {
      "root": "apps/admin-e2e",
      "sourceRoot": "apps/admin-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/admin-e2e/cypress.json",
            "tsConfig": "apps/admin-e2e/tsconfig.e2e.json",
            "devServerTarget": "admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "admin:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/admin-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "graphql-server": {
      "root": "apps/graphql-server",
      "sourceRoot": "apps/graphql-server/src",
      "projectType": "application",
      "prefix": "graphql-server",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/graphql-server",
            "main": "apps/graphql-server/src/main.ts",
            "tsConfig": "apps/graphql-server/tsconfig.app.json",
            "assets": ["apps/graphql-server/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/graphql-server/src/environments/environment.ts",
                  "with": "apps/graphql-server/src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "graphql-server:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/graphql-server/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/graphql-server/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/graphql-server"]
        }
      }
    },
    "api-graphql-resolvers": {
      "root": "libs/api/graphql/resolvers",
      "sourceRoot": "libs/api/graphql/resolvers/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api/graphql/resolvers/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api/graphql/resolvers"],
          "options": {
            "jestConfig": "libs/api/graphql/resolvers/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-graphql-schema": {
      "root": "libs/api/graphql/schema",
      "sourceRoot": "libs/api/graphql/schema/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api/graphql/schema/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api/graphql/schema"],
          "options": {
            "jestConfig": "libs/api/graphql/schema/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn graphql-codegen --config tools/graphql-codegen.yml"
              }
            ]
          }
        }
      }
    },
    "shared-types": {
      "root": "libs/shared/types",
      "sourceRoot": "libs/shared/types/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/types/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/types"],
          "options": {
            "jestConfig": "libs/shared/types/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-config": {
      "root": "libs/shared/config",
      "sourceRoot": "libs/shared/config/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/config/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/config"],
          "options": {
            "jestConfig": "libs/shared/config/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-data-access": {
      "root": "libs/api/data-access",
      "sourceRoot": "libs/api/data-access/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api/data-access/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api/data-access"],
          "options": {
            "jestConfig": "libs/api/data-access/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "yarn"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "admin"
}
