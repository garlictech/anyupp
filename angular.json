    "infrastructure-anyupp-backend-stack": {
      "root": "apps/infrastructure/anyupp-backend-stack",
      "sourceRoot": "apps/infrastructure/anyupp-backend-stack/src",
      "projectType": "application",
      "prefix": "infrastructure-anyupp-backend-stack",
      "architect": {
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/infrastructure/anyupp-backend-stack",
            "parallel": false,
            "commands": [
              "yarn sls package --stage='{args.stage}'",
              "yarn sst cdk synth"
            ]
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "deploy": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/infrastructure/anyupp-backend-stack",
            "parallel": false,
            "commands": [
              {
                "command": "yarn sst deploy --stage='{args.stage}'"
              },
              {
                "command": "cd ../../.. && yarn ts-node ./tools/fetch-configuration.ts '{args.app}' '{args.stage}'"
              }
            ]
          }
        },
        "remove": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/infrastructure/anyupp-backend-stack",
            "parallel": false,
            "commands": [
              {
                "command": "yarn sst remove --stage='{args.stage}'"
              }
            ]
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/infrastructure/anyupp-backend-stack/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/infrastructure/anyupp-backend-stack"
          ],
          "options": {
            "jestConfig": "apps/infrastructure/anyupp-backend-stack/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
