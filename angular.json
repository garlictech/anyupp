{
  "version": 1,
  "projects": {
    "admin": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/admin",
      "sourceRoot": "apps/admin/src",
      "prefix": "bgap",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/admin",
            "index": "apps/admin/src/index.html",
            "main": "apps/admin/src/main.ts",
            "polyfills": "apps/admin/src/polyfills.ts",
            "tsConfig": "apps/admin/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/admin/src/favicon.ico",
              "apps/admin/src/assets"
            ],
            "styles": [
              "apps/admin/src/styles.scss",
              "node_modules/@nebular/theme/styles/prebuilt/default.css",
              "node_modules/bootstrap/dist/css/bootstrap.css"
            ],
            "allowedCommonJsDependencies": [
              "fabric",
              "chart.js",
              "chartjs-plugin-datalabels",
              "lodash",
              "firebase/app",
              "@firebase",
              "@firebase/app",
              "@firebase/auth",
              "@firebase/component",
              "@firebase/database",
              "@firebase/functions",
              "@angular/common/locales",
              "zen-observable",
              "graphql-tag"
            ],
            "scripts": [],
            "fileReplacements": [
              {
                "replace": "./libs/shared/config/src/lib/firebase.config.json",
                "with": "./libs/shared/config/src/lib/dev/firebase.config.json"
              },
              {
                "replace": "./libs/shared/config/src/lib/firebase-service-account-key.json",
                "with": "./libs/shared/config/src/lib/dev/firebase-service-account-key.json"
              },
              {
                "replace": "./libs/shared/config/src/lib/config.json",
                "with": "./libs/shared/config/src/lib/dev/config.json"
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "./libs/shared/config/src/lib/firebase.config.json",
                  "with": "./libs/shared/config/src/lib/production/firebase.config.json"
                },
                {
                  "replace": "./libs/shared/config/src/lib/firebase-service-account-key.json",
                  "with": "./libs/shared/config/src/lib/production/firebase-service-account-key.json"
                },
                {
                  "replace": "./libs/shared/config/src/lib/config.json",
                  "with": "./libs/shared/config/src/lib/production/config.json"
                },
                {
                  "replace": "libs/admin/shared/config/src/lib/environments/environment.ts",
                  "with": "libs/admin/shared/config/src/lib/environments/environment.production.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "qa": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "./libs/shared/config/src/lib/firebase.config.json",
                  "with": "./libs/shared/config/src/lib/qa/firebase.config.json"
                },
                {
                  "replace": "./libs/shared/config/src/lib/firebase-service-account-key.json",
                  "with": "./libs/shared/config/src/lib/qa/firebase-service-account-key.json"
                },
                {
                  "replace": "./libs/shared/config/src/lib/config.json",
                  "with": "./libs/shared/config/src/lib/qa/config.json"
                },
                {
                  "replace": "libs/admin/shared/config/src/lib/environments/environment.ts",
                  "with": "libs/admin/shared/config/src/lib/environments/environment.qa.ts"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "admin:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "admin:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/admin/**/*.ts",
              "apps/admin/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/admin/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/admin"
          ]
        }
      }
    },
    "infrastructure-build-pipeline-stack": {
