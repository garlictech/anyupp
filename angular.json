{
  "version": 1,
  "projects": {
    "admin": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/admin",
      "sourceRoot": "apps/admin/src",
      "prefix": "bgap",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/admin",
            "index": "apps/admin/src/index.html",
            "main": "apps/admin/src/main.ts",
            "polyfills": "apps/admin/src/polyfills.ts",
            "tsConfig": "apps/admin/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/admin/src/favicon.ico", "apps/admin/src/assets"],
            "styles": [
              "apps/admin/src/styles.scss",
              "node_modules/@nebular/theme/styles/prebuilt/default.css",
              "node_modules/bootstrap/dist/css/bootstrap.css"
            ],
            "allowedCommonJsDependencies": [
              "fabric",
              "chart.js",
              "chartjs-plugin-datalabels",
              "lodash",
              "firebase/app",
              "@firebase",
              "@firebase/app",
              "@firebase/auth",
              "@firebase/component",
              "@firebase/database",
              "@firebase/functions",
              "@angular/common/locales"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/admin/src/environments/environment.ts",
                  "with": "apps/admin/src/environments/environment.prod.ts"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/firebase.config.json",
                  "with": "libs/shared/config/src/lib/config/prod/firebase.config.prod.json"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/stripe.config.json",
                  "with": "libs/shared/config/src/lib/config/prod/stripe.config.prod.json"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/service-account-key.json",
                  "with": "libs/shared/config/src/lib/config/prod/service-account-key.prod.json"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "qa": {
              "fileReplacements": [
                {
                  "replace": "apps/admin/src/environments/environment.ts",
                  "with": "apps/admin/src/environments/environment.qa.ts"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/firebase.config.json",
                  "with": "libs/shared/config/src/lib/config/qa/firebase.config.qa.json"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/stripe.config.json",
                  "with": "libs/shared/config/src/lib/config/qa/stripe.config.qa.json"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/service-account-key.json",
                  "with": "libs/shared/config/src/lib/config/qa/service-account-key.qa.json"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "apps/admin/src/environments/environment.ts",
                  "with": "apps/admin/src/environments/environment.staging.ts"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/firebase.config.json",
                  "with": "libs/shared/config/src/lib/config/staging/firebase.config.staging.json"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/stripe.config.json",
                  "with": "libs/shared/config/src/lib/config/staging/stripe.config.staging.json"
                },
                {
                  "replace": "libs/shared/config/src/lib/config/service-account-key.json",
                  "with": "libs/shared/config/src/lib/config/staging/service-account-key.staging.json"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "admin:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "admin:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/admin/**/*.ts", "apps/admin/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/admin/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/admin"]
        }
      }
    },
    "admin-e2e": {
      "root": "apps/admin-e2e",
      "sourceRoot": "apps/admin-e2e/src",
      "projectType": "application",
      "architect": {
