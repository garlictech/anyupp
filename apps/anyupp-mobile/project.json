{
  "projectType": "application",
  "root": "apps/anyupp-mobile",
  "sourceRoot": "apps/anyupp-mobile/lib",
  "targets": {
    "lint": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "apps/anyupp-mobile"
      }
    },
    "lint-ci": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "docker run --platform linux/amd64 --rm -v ${PWD}:/build -v /tmp/x:/root/.pub-cache --workdir /build ${AWS_ACCOUNT}.dkr.ecr.eu-west-1.amazonaws.com/anyupp-flutter flutter analyze",
        "cwd": "apps/anyupp-mobile"
      }
    },
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter clean",
        "cwd": "apps/anyupp-mobile"
      }
    },
    "build-schema": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "parallel": false,
        "command": "../../tools/build-schema-mobile-api.sh {args.env} {args.mode}",
        "cwd": "apps/crud-backend"
      },
      "outputs": ["apps/anyupp-mobile/graphql/"]
    },
    "pub-get": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "parallel": false,
        "command": "flutter pub get",
