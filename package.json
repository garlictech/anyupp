  "scripts": {
    "ng": "nx",
    "_postinstall": "node ./decorate-angular-cli.js && yarn ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "cucumber:report": "node ./tools/generate-cucumber-report.js",
    "cypress:merge:reports": "mochawesome-merge cyreport/mochawesome/*.json > cyreport/cypress-tests-report.json",
    "cypress:create:html:report": "marge --reportDir cyreport cyreport/cypress-tests-report.json",
    "cypress:generate:html:report": "yarn cypress:merge:reports && yarn cypress:create:html:report",
    "workspace-generator": "nx workspace-generator",
    "postinstall": "patch-package && ngcc --properties es2020 browser module main",
    "codegen:models": "if [ -d libs/api/graphql/schema/src/lib/models ]; then rm -r libs/api/graphql/schema/src/lib/models; fi; amplify codegen models; if [ -d apps/admin/src/models ]; then mv apps/admin/src/models libs/api/graphql/schema/src/lib/models; fi",
    "prepare": "husky install",
    "seed": "yarn ts-node --project ./tools/tsconfig.json -r tsconfig-paths/register ./tools/seed-execute.ts",
    "seed-yellow": "yarn ts-node --project ./tools/tsconfig.json -r tsconfig-paths/register ./tools/yellow-menusync.ts",
    "deleteAllTableData": "yarn ts-node --project ./tools/tsconfig.json -r tsconfig-paths/register ./tools/delete-all-table-data.ts",
    "reindexTables": "yarn ts-node --project ./tools/tsconfig.json -r tsconfig-paths/register ./tools/reindex-tables.ts",
    "unittest": "yarn nx run-many --target=test --all --exclude='integration-tests-angular' --exclude='integration-tests-universal' --exclude=shared-config --exclude=anyupp-gql-api --exclude=crud-gql-api",
    "alltest": "yarn nx run-many --skip-nx-cache --target=test --all --parallel --detectOpenHandles --forceExit --coverage=true && node ./tools/coverage-merger.js"
  },
