{
  "root": "libs/backend/reports",
  "sourceRoot": "libs/backend/reports/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/backend/reports/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/backend/reports"],
      "options": {
        "jestConfig": "libs/backend/reports/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:backend", "platform:nodejs"]
}
